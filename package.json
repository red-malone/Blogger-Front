  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/material": "^20.2.10",
    "@angular/platform-browser": "^21.0.1",
    "@angular/router": "^21.0.1",
    "dotenv": "^17.2.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
