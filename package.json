{
  "name": "blog-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "generate-env": "node ./scripts/generate-environment.js",
    "prestart": "node ./scripts/generate-environment.js",
    "prebuild": "node ./scripts/generate-environment.js",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
